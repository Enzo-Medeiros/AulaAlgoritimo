#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main(void){
     
int mat[3][3]={{1,2,3}, {4,5,6}, {7,8,9}};
int l,c; //normalmente i para linha e j para coluna

        printf("\nImprimindo a primeira linha: \n");
        for (l = 0; l < 3; l++)
        {
            printf(" %d ", mat[0][l]);
        }
        printf("\nImprimindo a primeira coluna: \n");
        for (c = 0; c < 3; c++)
        {
            printf(" %d ", mat[c][0]);
        }
        printf("\nImprimindo a matriz inteira: \n");
        for (l = 0; l < 3; l++)
        {
            for (c = 0; c < 3; c++)
            {
                printf(" |%d| ", mat[l][c]);
            }
        printf("\n");    
        }
        


}
