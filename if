#include <stdio.h>
int main(void){
 int idade=5;
    if(idade>18){
    printf("Maior\n");
    }
    if(idade<18){
        printf("Menor\n");
    }
    if(idade>=18){
        printf("Maior ou igual\n");
    }
    if(idade<=18){
        printf("Menor ou igual\n");
    }
    if(idade==18){
        printf("Igual\n");
    }
    if(idade!=18){
        printf("Diferente\n");
    }
}
